net: "examples/mnist/mnist_autoencoder.prototxt"
test_state: { stage: 'test-on-train' }
test_iter: 500
test_state: { stage: 'test-on-test' }
test_iter: 100
test_interval: 500
test_compute_loss: true

log_high_alpha: 2
log_low_alpha: -6
n_alphas: 33
explore_const: 1e-8
ducb_gamma: .99

display: 100
max_iter: 65000
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "examples/mnist/mnist_autoencoder_ducb_train"
# solver mode: CPU or GPU
solver_mode: GPU
solver_type: DUCB
