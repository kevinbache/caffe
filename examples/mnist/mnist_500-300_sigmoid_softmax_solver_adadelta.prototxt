net: "examples/mnist/mnist_500-300_sigmoid_softmax.prototxt"
test_state: { stage: 'test-on-train' }
test_iter: 250
test_state: { stage: 'test-on-test' }
test_iter: 50
test_interval: 250
test_compute_loss: true

momentum: 0.95
delta: 1e-8

display: 50
max_iter: 50000
snapshot: 10000
snapshot_prefix: "examples/mnist/mnist_500-300_sigmoid_softmax_ducb_train"
# solver mode: CPU or GPU
solver_mode: GPU
solver_type: ADADELTA
random_seed: 1